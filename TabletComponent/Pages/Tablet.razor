@page "/"

@inherits TabletBase

<head>
    <link rel="stylesheet" href="../css/tablet_style.css" />
</head>

<div id="@ActualScheme" class="page_container">

    <section class="tablet">
        <div class="tablet_container @current_wallpeper.Style">

            @if (show_main_screen == true)
            {
                <div class="screen">
                    <div class="screen_container">
                        <div class="apps">
                            <div class="apps_container">

                                <div @onclick="OpenChatApp" class="app">
                                    <img src="../images/messenger.png" alt="messenger" />
                                    <p>Chat with me</p>
                                </div>

                                <div @onclick="OpenGalleryApp" class="app">
                                    <img src="../images/gallery.png" alt="gallery" />
                                    <p>Projects gallery</p>
                                </div>

                                <div @onclick="OpenOptions" class="app">
                                    <img src="../images/options.png" alt="options" />
                                    <p>Options</p>
                                </div>

                                <div @onclick="OpenInternetApp" class="app">
                                    <img src="../images/internet.png" alt="internet" />
                                    <p>Internet</p>
                                </div>

                                <div @onclick="OpenLiveApp" class="app">
                                    <img src="../images/live.png" alt="internet" />
                                    <p>See me live</p>
                                </div>


                                <div @onclick="OpenSkillsApp" class="app">
                                    <img src="../images/skills.png" alt="skills" />
                                    <p>Skills</p>
                                </div>

                            </div>
                        </div>

                    </div>
                </div>
            }

            @if (show_chat_app == true)
            {
                <div class="chat_app">
                    <div class="chat_app_container">

                        <div class="chat_logs">
                            <div class="chat_logs_container">
                                @foreach (var o in asked_questions)
                                {
                                    <div class="out_txt">
                                        <p>@o.Question </p>
                                    </div>


                                    <div class="out_txt">
                                        <p>@o.Answer </p>
                                    </div>
                                }
                            </div>
                        </div>

                        <div class="questions">
                            <div class="questions_container">
                                @foreach (var o in new_questions)
                                {
                                    <p @onclick="@(e => AskQuestion(o.Id))">@o.Question</p>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            }

